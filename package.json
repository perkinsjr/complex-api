{
  "name": "complex-api",
  "version": "1.0.0",
  "description": "A complex Express API with TypeScript and OpenAPI spec generation for Unkey deployment demo",
  "main": "dist/index.js",
  "scripts": {
    "generate:openapi": "node scripts/generate-openapi.js",
    "watch:openapi": "nodemon --watch src/routes --ext ts --exec \"npm run generate:openapi\"",
    "prebuild": "npm run generate:openapi && npm run validate:openapi",
    "build": "tsc",
    "prestart": "npm run generate:openapi && npm run validate:openapi",
    "start": "node dist/index.js",
    "predev": "npm run generate:openapi",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:full": "concurrently \"npm run watch:openapi\" \"npm run dev\"",
    "validate:openapi": "node scripts/validate-openapi.js",
    "postgenerate:openapi": "npm run validate:openapi",
    "test:endpoints": "node scripts/test-endpoints.js",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "openapi",
    "unkey"
  ],
  "author": "Unkey",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "faker": "^5.5.3",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/faker": "^5.5.8",
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
