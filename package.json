{
  "name": "complex-api",
  "version": "1.0.0",
  "description": "A complex Express API with TypeScript and OpenAPI spec generation for Unkey deployment demo",
  "main": "dist/index.js",
  "scripts": {
    "generate:openapi": "node scripts/generate-openapi.js",
    "prebuild": "npm run generate:openapi",
    "build": "tsc",
    "prestart": "npm run generate:openapi",
    "start": "node dist/index.js",
    "predev": "npm run generate:openapi",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "openapi",
    "unkey"
  ],
  "author": "Unkey",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.1",
    "faker": "^5.5.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/compression": "^1.7.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/js-yaml": "^4.0.9",
    "@types/uuid": "^9.0.7",
    "@types/faker": "^5.5.8",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "ts-node-dev": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
